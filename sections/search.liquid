{{ 'section-search.css' | asset_url | stylesheet_tag }}

<section class="search faq-search">
  <div class="container">
    <div class="search__content">
      {% if section.settings.title != blank %}
        <h2 class="search__title">{{ section.settings.title }}</h2>
      {% endif %}
      {% render 'search' %}
    </div>
  </div>
</section>

{% schema %}
{
  "name": "t:sections.search.name",
  "tag": "section",
  "class": "section",
  "settings": [
    {
      "type": "text",
      "id": "title",
      "default": "Search",
      "label": "t:sections.search.settings.title.label"
    }
  ],
  "presets": [
    {
      "name": "t:sections.search.presets.name"
    }
  ]
}
{% endschema %}
